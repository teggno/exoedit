{
  "name": "exoedit",
  "displayName": "exoedit",
  "description": "Exosite Widget Script Editor",
  "version": "0.1.0",
  "publisher": "teggno",
  "engines": {
    "vscode": "^0.10.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.listActions"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.listActions",
        "title": "Exoedit"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
  },
  "devDependencies": {
    "typescript": "^1.6.2",
    "vscode": "0.10.x"
  },
  "dependencies": {
    "request": "^2.69.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teggno/exoedit.git"
  }
}
